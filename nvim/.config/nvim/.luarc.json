{
    "diagnostics": {
        "globals": ["vim"]
    },
    "workspace": {
        "library": [
            "${3rd}/luv/library",
            "/usr/local/share/nvim/runtime/lua",
            "~/.local/share/nvim/lazy/*/lua"
        ],
        "checkThirdParty": false
    },
    "completion": {
        "callSnippet": "Replace"
    },
    "telemetry": {
        "enable": false
    }
}
